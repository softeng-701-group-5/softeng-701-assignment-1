{
  "name": "feedr-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "run-p server startReact",
    "startReact": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --updateSnapshot",
    "eject": "react-scripts eject",
    "server": "node proxy-server --exec nodemon",
    "dev": "run-p server start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "fg-loadcss": "^2.1.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "querystring": "^0.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-github-login": "^1.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-stack-grid": "^0.7.1",
    "react-twitter-login": "^1.1.2",
    "react-use-googlelogin": "^0.11.2",
    "react-waypoint": "^9.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.0"
  }
}
